{
  "name": "berlysia-net",
  "version": "0.0.0",
  "description": "",
  "author": "berlysia <berlysia@gmail.com>",
  "main": "index.js",
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "lint": "eslint 'src/**/*.{js,ts,jsx,tsx}'",
    "lint:fix": "npm-run-all 'lint -- --fix'",
    "format": "prettier --write '**/*.{json,md}'",
    "test": "jest",
    "clean": "del dist",
    "dev": "next",
    "build": "run-s build:seed build:next",
    "build:next": "next build",
    "build:seed": "ts-node src/seeds/build.ts",
    "export": "next export",
    "start": "next start",
    "deploy": "yarn build && yarn export",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@berlysia/eslint-config": "^1.2.0",
    "@berlysia/tsconfig": "^0.0.1",
    "@types/babel__core": "^7.1.3",
    "@types/eslint": "^6.1.3",
    "@types/fs-extra": "^9.0.11",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/node": "^15.0.2",
    "@types/prettier": "^1.19.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/rebass": "^4.0.3",
    "@types/webpack": "^4.41.1",
    "@types/webpack-dev-server": "^3.9.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.0.6",
    "del-cli": "^3.0.0",
    "eslint": "^7.16.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.13",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.1.2",
    "ts-jest": "^24.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@rebass/preset": "^4.0.5",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.15",
    "next": "^10.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rss-parser": "^3.12.0"
  }
}
