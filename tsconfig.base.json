{
  "extends": "@tsconfig/next/tsconfig.json",
  "compilerOptions": {
    "module": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "target": "esnext",
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "isolatedModules": true,
    "declaration": false,
    "noEmitOnError": false,
    "incremental": true,
    "noUnusedLocals": false
  }
}